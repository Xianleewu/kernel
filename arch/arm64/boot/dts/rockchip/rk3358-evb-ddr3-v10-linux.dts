/dts-v1/;

#include "rk3358-evb-ddr3.dtsi"
#include "rk3358-linux.dtsi"

/ {
	model = "Rockchip linux RK3358 EVB DDR3 board";
	compatible = "rockchip,rk3358-evb-ddr3-v10-linux", "rockchip,px30", "rockchip,rk3358";

	memory: memory {
		device_type = "memory";
		reg = <0x00000000 0x0 0x0 0x20000000>;
	};

	ramdisk: ramdisk {
		compatible = "rockchip,ramdisk";
		memory-region = <&ramdisk_r>;
	};

	reserved-memory {
		#address-cells = <2>;
		#size-cells = <2>;
		ranges;
		atf@0 {
			reg = <0x0 0x00000 0x0 0x200000>;
			no-map;
		};

		atf@200000 {
			reg = <0x0 0x200000 0x0 0x8000>;
		};

		ramdisk_r: ramdisk_r@4000000 {
			reg = <0x0 0x4000000 0x0 0x2000000>;
			//no-map;
		};
	};
};
